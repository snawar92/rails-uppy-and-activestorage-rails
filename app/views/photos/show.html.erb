<div class="page-header">
  <%= link_to photos_path, class: 'btn btn-default' do %>
    All Photos
  <% end %>
  <%= link_to edit_photo_path(@photo), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show photo</h1>
</div>

<% if @photo.image.attached? %>
  <%= image_tag @photo.image %>
<% end %>
